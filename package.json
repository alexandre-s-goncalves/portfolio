{
  "name": "portfolio",
  "version": "0.0.1s",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json,md}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\" && eslint --config .eslintrc.cjs \"src/**/*.{ts,tsx,js,jsx}\" --fix --no-ignore",
    "format:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\" && eslint --config .eslintrc.cjs \"src/**/*.{ts,tsx,js,jsx}\" --fix --no-ignore",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --config .eslintrc.cjs --fix"
    ]
  },
  "dependencies": {
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.1.4",
    "react-router-dom": "^7.9.4",
    "styled-components": "^6.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-jest": "^29.6.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-styled-components": "^7.2.0",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
